{
  "name": "github-api-task",
  "version": "1.0.0",
  "description": "This is a script for updating an npm module version in the GitHub repository. The result of running this script is a pull-request with updated package.json file",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "start": "node --no-warnings=ExperimentalWarning --loader ts-node/esm --env-file=.env src/index.ts"
  },
  "author": "Yana Omelchenko",
  "license": "ISC",
  "dependencies": {
    "octokit": "^4.0.2"
  },
  "devDependencies": {
    "@octokit/types": "^13.5.0",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/*.test.ts"
    ]
  }
}
